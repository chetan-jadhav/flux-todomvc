{
  "name": "flux-shell",
  "version": "1.0.0",
  "description": "Basic shell application for the Flux architecture",
  "repository": "https://github.com/facebook/flux",
  "author": "Kyle Davis",
  "license": "BSD-3-Clause",
  "main": "bundle.js",
  "scripts": {
    "build": "webpack ./src/root.js ./bundle.js",
    "watch": "webpack ./src/root.js ./bundle.js --watch",
    "start": "webpack-dev-server --hot",
    "test": "jest"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "flux": "^3.1.2",
    "immutable": "^3.8.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack-dev-server": "^2.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-syntax-jsx": "^6.5.0",
    "babel-plugin-syntax-object-rest-spread": "^6.5.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-plugin-transform-regenerator": "^6.5.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "jest": "^19.0.2",
    "webpack": "^1.13.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/tests/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "rootDir": "",
    "roots": [
      "<rootDir>"
    ]
  }
}
